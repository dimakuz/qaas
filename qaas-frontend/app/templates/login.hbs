<h2 id="title">Login</h2>
{{#if error}}
    <div class="alert alert-danger" role="alert">{{error}}</div>
{{/if}}
<form>
    <div class="form-group">
        <label for="queueName">User name</label>
        {{input type="name" class="form-control" placeholder="name" value=model.name}}
    </div>
    <div class="form-group">
        <label for="password">Password</label>
        {{input type="password" class="form-control" placeholder="Password" value=model.password}}
    </div>
    <button {{action 'create'}} class="btn btn-default">Log In</button>
</form>
{{outlet}}
