<h2 id="title">Create a queue</h2>
<form>
    <div class="form-group">
        <label for="queueName">Queue name</label>
      {{input type="name" class="form-control" placeholder="My fantastic queue" value=model.name}}
    </div>
    <div class="form-group">
        <label for="password">Password</label>
      {{input type="password" class="form-control" placeholder="Password" value=model.secret}}
    </div>
    <div class="checkbox">
        <label>
            <input type="checkbox"> Make queue public
        </label>
    </div>
    <button {{action 'create'}} class="btn btn-default">Submit</button>
</form>
{{outlet}}
